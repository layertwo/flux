{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
      "(^|/)clusters/.+\\.ya?ml$"
      ],
      "matchStrings": [
        "image(?:Name|): ['\"]?(?<depName>.+?):(?<currentValue>.+)['\"]?"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
